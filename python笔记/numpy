import numpy as np
#导入矩阵
array = np.array([[]1,2,3],[4,5,6])

属性
#维度
array.ndim
#形状
array.shape
#大小
array.size


#定义类型
a=np.array([1,3,4],dtype=np.int)   #这里可以改为np.int64   np.float

#定义0矩阵 3行4列
a=np.zeros((3,4))
#定义1矩阵
a=np.ones((3,4),dtype=int)
a=np.ones((3,4),dtype=int)*2    #变成2

#空
b=np.empty((3,4))


#定义序列
a=np.arange(10)
#改变形状
a=np.arange(10).reshpe((2,5))
